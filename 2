var clickhere=document.getElementById('submit');
clickhere.addEventListener('click',function(){
    passworddemo();
passwordformValidation();

});
function passwordformValidation(){
    var clickhe=document.getElementById('input').value;
if (clickhe.trim()==""){
    alert("enter your password it's must");
}
else{
    alert("submition is successfull");
}
}
function passworddemo(){
    var clickher=document.getElementById('demo').value;
    if(clickher.trim()==""){
        alert("First Name has to be entered");
    }
    
}
